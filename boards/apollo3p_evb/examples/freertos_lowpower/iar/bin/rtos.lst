    193          //*****************************************************************************
    194          void
    195          run_tasks(void)
    196          {
    197              //
    198              // Set some interrupt priorities before we create tasks or start the scheduler.
    199              //
    200              // Note: Timer priority is handled by the FreeRTOS kernel, so we won't
    201              // touch it here.
    202              //
    203          
    204              //
    205              // Create essential tasks.
    206              //
    207              xTaskCreate(setup_task, "Setup", 128, 0, 3, &xSetupTask);
    208          
    209          
